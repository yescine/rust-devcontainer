{
  "name": "Rust Dev Container",
  "context": "..",
  "dockerFile": "../Dockerfile.dev",
  "mounts": ["source=${localWorkspaceFolder},target=/app,type=bind"],
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "vadimcn.vscode-lldb",
        "mutantdino.resourcemonitor",
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "serayuzgur.crates"
      ]
    }
  }
}
